{
	"name": "tiktok_volume",
	"version": "0.1",
	"description": "Build an Extension!",
	"manifest_version": 2,


	"background": {
		"scripts": ["background.js"]
	},

	"content_scripts": [
		{
			"matches": ["https://www.tiktok.com/*"],
			"js": ["contentScript.js"],
			"run_at": "document_start"
		}
	],

	"browser_action": {
		"default_icon": {
			"16": "hello_extensions.png"
		}
	}

}